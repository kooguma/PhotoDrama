<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MovieMakerGLSurfaceView">
        <attr name="screenRatioX" format="integer"/>
        <attr name="screenRatioY" format="integer"/>
    </declare-styleable>

    <declare-styleable name="ScrollSelectView">
        <attr name="imageShowHeight" format="dimension"/>
        <attr name="indicatorRectangleHeight" format="dimension"/>
        <attr name="indicatorTriangleHeight" format="dimension"/>
        <attr name="indicatorBottomMargin" format="dimension"/>
        <attr name="indicatorWidth" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="MusicClipView">
        <attr name="progressbarColor" format="color"/>
        <attr name="progressbarSelectedColor" format="color"/>
        <attr name="dotColor" format="color"/>
        <attr name="progressMax" format="integer"/>
        <attr name="roundCorner" format="boolean"/>
    </declare-styleable>

</resources>