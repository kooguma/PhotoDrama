<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
               android:shape="rectangle">

            <size
                android:width="10dp"
                android:height="10dp"/>

            <corners android:radius="5dp"/>

            <stroke
                android:width="1dp"
                android:color="#61d898"></stroke>

        </shape>
    </item>

    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
               android:shape="rectangle">

            <size
                android:width="6dp"
                android:height="6dp"/>

            <corners android:radius="3dp"/>

            <solid android:color="#61d898"/>

        </shape>
    </item>
</layer-list>
